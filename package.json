{
  "name": "malas",
  "version": "1.2.0",
  "description": "AI-powered CLI assistant that runs commands and writes files based on user prompts.",
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/index.js",
  "bin": {
    "malas": "./dist/index.js"
  },
  "release": {
    "branches": [
      "master"
    ]
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "dev": "tsx src/index.ts",
    "build": "tsup src/index.ts --format cjs --minify",
    "test": "vitest",
    "prepublishOnly": "pnpm run build"
  },
  "author": "Luki",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^22.7.4",
    "tsup": "^6.0.0",
    "typescript": "^5.0.0",
    "vitest": "^0.32.0"
  },
  "dependencies": {
    "@google/generative-ai": "^0.21.0",
    "commander": "^12.1.0"
  }
}
