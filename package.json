{
  "name": "malas",
  "version": "1.6.0",
  "description": "AI-powered CLI assistant that runs commands and writes files based on user prompts.",
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/index.mjs",
  "bin": {
    "malas": "./dist/index.mjs"
  },
  "repositoryUrl": "https://github.com/lzif/malas.git",
  "release": {
    "branches": [
      "master"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      [
        "@semantic-release/npm",
        {
          "npmPublish": true
        }
      ],
      [
        "@semantic-release/github",
        {
          "assets": [
            {
              "path": "dist/*.js",
              "label": "JavaScript distribution files"
            }
          ]
        }
      ],
      [
        "@semantic-release/git",
        {
          "assets": [
            "package.json",
            "CHANGELOG.md"
          ],
          "message": "chore(release): ${nextRelease.version} [skip ci]"
        }
      ]
    ]
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "dev": "tsx src/index.ts",
    "build": "tsup src/index.ts --format cjs,esm --minify --clean",
    "test": "vitest run",
    "prepublishOnly": "pnpm run build"
  },
  "author": "lzif",
  "license": "MIT",
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.0",
    "@semantic-release/npm": "^12.0.1",
    "@types/git-diff": "^2.0.7",
    "@types/node": "^22.7.4",
    "semantic-release": "^24.2.0",
    "tsup": "^6.0.0",
    "typescript": "^5.0.0",
    "vitest": "^0.32.0"
  },
  "dependencies": {
    "@google/generative-ai": "^0.21.0",
    "chalk": "^5.3.0",
    "cli-highlight": "^2.1.11",
    "commander": "^12.1.0",
    "git-diff": "^2.0.6",
    "ora": "^8.1.1"
  }
}
