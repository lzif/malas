{
  "name": "malas",
  "version": "2.0.0",
  "description": "Deterministic CLI generator for repetitive dev workflows. Simple, fast, no AI.",
  "type": "module",
  "bin": {
    "malas": "./dist/index.js"
  },
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/lzif/malas.git"
  },
  "author": "lzif",
  "license": "MIT",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "dev": "tsx src/index.ts",
    "build": "tsup src/index.ts --format esm --clean",
    "test": "vitest run",
    "prepublishOnly": "pnpm run build"
  },
  "dependencies": {
    "commander": "^12.1.0",
    "chalk": "^5.3.0"
  },
  "devDependencies": {
    "@types/node": "^22.7.4",
    "semantic-release": "^24.2.0",
    "@semantic-release/commit-analyzer": "^11.0.0",
    "@semantic-release/release-notes-generator": "^12.0.0",
    "@semantic-release/npm": "^12.0.1",
    "@semantic-release/github": "^11.0.0",
    "tsup": "^8.0.1",
    "tsx": "^4.19.2",
    "typescript": "^5.5.4",
    "vitest": "^1.6.0"
  }
}
